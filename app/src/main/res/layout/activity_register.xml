<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        tools:context=".ui.login.RegisterActivity" >

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress"
            android:autofillHints="@string/prompt_email"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="154dp"
            android:layout_marginBottom="34dp" app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword"
            android:layout_marginEnd="137dp" app:layout_constraintEnd_toEndOf="parent"
            android:text="@string/prompt_email"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword"
            android:layout_marginTop="61dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" android:layout_marginBottom="28dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            android:layout_marginEnd="139dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.518"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"
            android:layout_marginEnd="137dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="55dp" app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/editTextTextPostalAddress"
            android:autofillHints="@string/home_address"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
            android:layout_marginTop="41dp" app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
            android:layout_marginEnd="134dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="239dp"
            app:layout_constraintHorizontal_bias="0.437"/>
    <TextView
            android:text="@string/password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            android:layout_marginTop="62dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" android:layout_marginEnd="302dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="14dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="51dp"/>
    <TextView
            android:text="@string/password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            android:layout_marginTop="62dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress" android:layout_marginEnd="302dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="14dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="51dp"/>
    <TextView
            android:text="@string/confirm_password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:layout_marginBottom="6dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"
            android:layout_marginEnd="242dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="31dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword"/>
    <TextView
            android:text="@string/home_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"
            android:layout_marginTop="27dp" app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
            android:layout_marginEnd="269dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="50dp" app:layout_constraintBottom_toTopOf="@+id/editTextTextPostalAddress"/>
    <Button
            android:text="@string/register"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/confirm_register"
            android:layout_marginTop="70dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPostalAddress"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="144dp"
            android:layout_marginEnd="179dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="76dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"/>
</androidx.constraintlayout.widget.ConstraintLayout>